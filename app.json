{
  "expo": {
    "name": "Veya",
    "slug": "veya-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "veya",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0F"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.veya.app",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "Veya needs access to your microphone to convert your voice to text for cosmic consultations.",
        "NSSpeechRecognitionUsageDescription": "Veya uses speech recognition to transcribe your questions for the stars."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0A0A0F"
      },
      "package": "com.veya.app",
      "permissions": ["RECORD_AUDIO"]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-speech-recognition"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "bc189d9b-4f4f-403a-90dc-256ba92e24b5"
      }
    },
    "owner": "shubham987654",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/bc189d9b-4f4f-403a-90dc-256ba92e24b5"
    }
  }
}
